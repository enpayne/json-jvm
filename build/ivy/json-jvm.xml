<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0">
	<info
		organisation="${maven.group}"
		module="${maven.artifact}"
		revision="${version}"
		status="release"
		default="true" />
	<configurations>
		<conf name="default" visibility="public" />
	</configurations>
	<dependencies>
		<dependency org="com.threecrickets.scripturian" name="scripturian" rev="1.1+" />
		<dependency org="org.mozilla" name="rhino" rev="1.7+" />
		<!-- <dependency org="jdk.nashorn" name="nashorn-backport" rev="0.1+" /> -->
	</dependencies>
</ivy-module>
